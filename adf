  function timebg(h) {
        const timebet;
        if (h < 4) {
            timebet = "midnight";
        }
        else if (h < 8) {
            timebet = "dawn";
        }
        else if (h < 12) {
            timebet = "morning"
        }
        else if (h < 16) {
            timebet = "afternoon"
        }
        else if (h < 20) {
            timebet = "evening"
        }
        else {
            timebet = "night";
        }
        return timebet;
    }